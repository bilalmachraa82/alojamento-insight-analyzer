# Supabase Configuration
VITE_SUPABASE_URL=your_supabase_project_url
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key

# Hugging Face API Configuration (for Sentiment Analysis)
# Get your API key at: https://huggingface.co/settings/tokens
VITE_HUGGINGFACE_API_KEY=your_huggingface_api_key

# Apify API Configuration
VITE_APIFY_API_KEY=your_apify_api_key

# Sentry Configuration (Error Tracking and Performance Monitoring)
# Get your DSN from: https://sentry.io/settings/[org]/projects/[project]/keys/
# The DSN is safe to expose in the client-side code
VITE_SENTRY_DSN=your_sentry_dsn_here

# Sentry Build Configuration (for source map upload - DO NOT commit these!)
# These are used during the build process to upload source maps
# Get auth token from: https://sentry.io/settings/account/api/auth-tokens/
# Required scopes: project:releases, project:write
SENTRY_ORG=your_sentry_organization_slug
SENTRY_PROJECT=your_sentry_project_slug
SENTRY_AUTH_TOKEN=your_sentry_auth_token

# Google Analytics 4 Configuration
# Get your Measurement ID from: https://analytics.google.com/
# Navigate to: Admin > Data Streams > Web > Measurement ID
# Format: G-XXXXXXXXXX
VITE_GA4_MEASUREMENT_ID=your_ga4_measurement_id

# Application Metadata (for Sentry tags)
VITE_APP_VERSION=0.0.0
VITE_APP_BUILD=local

# Other API Keys
# Add any additional API keys your project requires below
